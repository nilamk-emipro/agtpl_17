        <div class="col">
            <button class="button_package_spitting btn btn-block btn-primary mb-4 w-100"
                    t-on-click="() => this.actionService.doAction('package_splitting_main_screen')">
                Package Spitting
            </button>
        </div>
    </xpath>
    </t>
</templates>